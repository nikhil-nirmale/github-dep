type Mutation{
createBook(book:BookInput):Book
}


type Query {
allBooks:[Book]
getBook(bookId:Int):Book
}

type Subscription {
  notifyBookAdded: [Book]
}

type Book {
id: ID!
title: String
description:String
author:String
price:Float
pages:Int
}

input BookInput{
id: Int
title: String
description:String
author:String
price:Float
pages:Int
}